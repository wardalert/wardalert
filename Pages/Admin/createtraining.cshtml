@page
@model wardalert.Pages.Admin.createtrainingModel
@{
    Layout = "_AdminLayout";
}




<form method="post" class="ff">
    <div class="form-group">
        <label for="TrainingId">Training ID</label>
        <input type="number" asp-for="TrainingId" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="Title">Training Title</label>
        <input type="text" asp-for="Title" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="Description">Description</label>
        <input type="text" asp-for="Description" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="Status">Status</label>
        <input type="text" asp-for="Status" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="Time">Time</label>
        <input type="datetime-local" asp-for="Time" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="StartDate">Start Date</label>
        <input type="date" asp-for="StartDate" required class="form-control" />
    </div>

    <div class="form-group">
        <label for="EndDate">End Date</label>
        <input type="date" asp-for="EndDate" required class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Upload</button>
</form>


