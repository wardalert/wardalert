@page
@model wardalert.Pages.EventsModel
@{
}


<h2>Event List</h2>

<table border="1" cellpadding="10">
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Uploaded At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Events)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.UploadedAt.ToString("yyyy-MM-dd HH:mm")</td>
            </tr>
        }
    </tbody>
</table>
